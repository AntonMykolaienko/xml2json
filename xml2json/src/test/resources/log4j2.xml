<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="Xml2Json">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %c{5}:%L: %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="all">
            <appender-ref ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>